@model Lab.EF.MVC.Models.ShippersView

<h1>Crear Nuevo Shipper</h1>

@using (Html.BeginForm("Insert", "Shippers", FormMethod.Post, new { id = "shipperForm" }))
{
    <div class="form-group">
        @Html.LabelFor(shipper => shipper.CompanyName)
        @Html.TextBoxFor(shipper => shipper.CompanyName, new { @class = "form-control", id = "CompanyName" })
    </div>

    <div class="form-group">
        @Html.LabelFor(shipper => shipper.Phone)
        @Html.TextBoxFor(shipper => shipper.Phone, new { @class = "form-control", type = "text" })


    </div>
    <button type="submit" class="btn btn-primary" id="add">Guardar</button>
}

@section scripts {
    <script src="~/Scripts/ValidacionServicio.js"></script>
    <script src="~/Scripts/ShipperScript.js"></script>
}